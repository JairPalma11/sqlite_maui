<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:pages="clr-namespace:SQliteMAUI.Pages"
            x:Class="SQliteMAUI.Pages.ContactsTabbedPage">

    <NavigationPage
        Title="Contactos"
        IconImageSource="person_icon">
        <x:Arguments>
            <pages:ContactsPage />
        </x:Arguments>
    </NavigationPage>

    <NavigationPage
        Title="SQL"
        IconImageSource="sql_icon">
        <x:Arguments>
            <pages:SQLiteOperationsPage />
        </x:Arguments>
    </NavigationPage>
</TabbedPage>
